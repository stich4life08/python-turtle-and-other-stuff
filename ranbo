import turtle#imports python vizual
import time 
t = turtle.Turtle()
s = turtle.Screen()
s.bgcolor ("blue")
t.color('white')
t.speed(10) 
#t.hideturtle() 
t.width(10)
def brs3():
	t.color('#9D8E3C')
	for count in range(4):
		t.fd(10)
		t.rt(90)
