def ranbo():
	brs2()
	t.fd(18)
	bls()
	t.fd(18)
	brs2()
	t.fd(18)
	bls()
	t.fd(18)
	bls()
	t.fd(18)
	brs2()
	t.fd(18)
	grs()
	t.fd(18)
	brs2()
	t.color('red')
	t.pu()
	t.fd(10)
	t.rt(90)
	t.fd(10)
	t.lt(90)
	t.bk(138)
	t.rt(90)
	t.fd(4)
	t.lt(90)
	t.pd()
	brs3()
	t.fd(18)
	brs3()
	t.fd(18)
	rs2()
	t.fd(18)
	gs2()
	

	
	
	



