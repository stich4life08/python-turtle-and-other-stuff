def popbob():
	t.width(10)
	s.bgcolor ("blue")
	def ws():
		t.color('#D6D2D1')
		for count in range(4):
			t.fd(10)
			t.rt(90)
	def ws1():
		t.color('#FEFEFE')
		for count in range(4):
			t.fd(10)
			t.rt(90)
	def bs():
		t.color('#010101')
		for count in range(4):
			t.fd(10)
			t.rt(90)

	ws()
	t.fd(18)
	for count in range(5):
		ws1()
		t.fd(18)
	ws1()
	t.fd(18)
