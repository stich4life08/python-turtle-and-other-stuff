import turtle
import time
t = turtle.Turtle()
s = turtle.Screen()
s.bgcolor ("white")
t.color('#00FF40')
t.speed(10)
t.width(10)
def sig():
	t.hideturtle()
	t.width(5)
	t.fd(5)
	t.lt(245)
	t.fd(6)
	t.color("#00FF99")
	t.rt(245)
	t.bk(5)
	t.color("#004CFF")
	t.pu()
	t.rt(90)
	t.fd(3)
	t.pd()
	t.lt(45)
	t.fd(2)
def techno():
	t.width(10)
	def y1():
		t.color('#DBCC00')
