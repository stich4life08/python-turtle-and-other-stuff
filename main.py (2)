def printer():
	t.hideturtle()
	s.bgcolor("blue")
	t.color("black")
	t.speed(10)
	t.width(3)
	for count in range(10):
		t.fd(100)
		t.pu()
		t.rt(90)
		t.fd(3)
		t.lt(90)
		t.pd()
		t.bk(100)
	time.sleep(1)
	t.clear()
	
def flag():
	t.hideturtle()
	s.bgcolor("black")
	t.color("red")
	t.speed(10)
	t.width(3)
	t.goto(0,0)
	t.fd(100)
	t.pu()
	t.rt(90)
	t.fd(3)
	t.lt(90)
	t.pd()
	t.bk(100)
	t.pu()
	t.rt(90)
	t.fd(3)
	t.lt(90)
	t.pd()
	t.fd(100)
	t.pu()
	t.rt(90)
	t.fd(3)
	t.lt(90)
	t.pd()
	t.bk(100)
	t.color("orange")
	t.pu()
	t.rt(90)
	t.fd(3)
	t.pd()
	t.lt(90)
	t.fd(100)
	t.pu()
	t.rt(90)
	t.fd(3)
	t.lt(90)
	t.pd()
	t.bk(100)
	t.pu()
	t.rt(90)
	t.fd(3)
	t.lt(90)
	t.pd()
	t.fd(100)
	t.pu()
	t.rt(90)
	t.fd(3)
	t.lt(90)
	t.pd()
	t.bk(100)
