			t.rt(90)
	def bl1():
		t.color('#020001')
		for count in range(4):
			t.fd(10)
			t.rt(90)
	
	y1()
	t.fd(18)
	p1()
	t.fd(18)
	y1()
	t.fd(18)
	p1()
	t.fd(18)
	p1()
	t.fd(18)
	y1()
	t.fd(18)
	p1()
	t.fd(18)
	y1()
	t.color('red')
	t.pu()
	t.fd(10)
	t.rt(90)
	t.fd(10)
	t.lt(90)
	t.bk(136)
	t.rt(90)
	t.fd(4)
	t.lt(90)
	t.pd()
	y1()
	t.fd(18)
	y1()
	t.fd(18)
	b2()
	t.fd(18)
	y1()
	t.fd(18)
	b1()
	t.fd(18)
	y1()
	t.fd(18)
	y1()
	t.fd(18)
	y1()
	t.color('red')
	t.pu()
	t.fd(10)
	t.rt(90)
	t.fd(10)
	t.lt(90)
	t.bk(136)
	t.rt(90)
	t.fd(4)
	t.lt(90)
	t.pd()	
	for count in range(7):
		y1()
		t.fd(18)
	y1()
	t.color('red')
