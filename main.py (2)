import turtle
t = turtle.Turtle()
s = turtle.Screen()
s.bgcolor ("white")
t.color('white')
t.speed(10)
t.width(10)

def techno():
	def y1():
		t.color('#DBCC00')
		for count in range(4):
			t.fd(10)
			t.rt(90)
	
	def p1():
		t.color('#EC959B')
		for count in range(4):
			t.fd(10)
			t.rt(90)
	def p2():
		t.color('#9C5052')
		for count in range(4):
			t.fd(10)
			t.rt(90)
	def p3():
		t.color('#EE9EA1')
		for count in range(4):
			t.fd(10)
			t.rt(90)
